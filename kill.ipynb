    "enc = tiktoken.get_encoding(\"gpt2\")\n",
    "\n",
    "with open('test.txt', 'r', encoding='utf-8') as f:\n",
    "    text = f.read()\n",
    "\n",
    "    # set up encoding\n",
    "    a = enc.encode(text) \n",
    "\n",
    "print(len(set(a)))"
